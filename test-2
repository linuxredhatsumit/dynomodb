      https_listener_index = 0
      priority             = 2
      actions = [{
        type               = "forward"
        target_group_index = 1
      }]

      conditions = [{
        host_headers  = ["insurance-internal.uat.kotak811.com"],
        path_patterns = ["/api/internal/insurance/*"]
      }]
    },
  ]
  target_groups = [
    {
      name                 = "uat-ghi-frontend-tg"
      backend_protocol     = "HTTP"
      backend_port         = 30721
      target_type          = "instance"
      deregistration_delay = 300
      health_check = {
