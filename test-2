        content_type = "text/plain"
        message_body = "Fixed message"
        status_code  = "200"
      }
    }
  ]
  https_listener_rules = [
    {
      https_listener_index = 0
      priority             = 1
      actions = [{
        type               = "forward"
        target_group_index = 0
      }]

      conditions = [{
        host_headers  = ["ghi.uat.kotak811.com", "ghi.uat.kotak811.bank.in"],
        path_patterns = ["/*"]
      }]
    },
